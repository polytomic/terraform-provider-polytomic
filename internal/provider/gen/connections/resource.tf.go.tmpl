resource "{{ .Resource }}" "{{ .Name }}" {
  organization = polytomic_organization.acme.id
  name         = "Acme, Inc"
  configuration = {
   {{- range .Attributes }}
   {{- if eq .Type "string" }}
    {{ .Name }} = "{{ .Example }}"
   {{- else }}
    {{ .Name }} = {{ .Example }}
   {{- end -}}
    {{ end }}
  }
}

