resource "{{ .Resource }}" "{{ .Name }}" {
  organization = polytomic_organization.acme.id
  name         = "example"
  configuration = {
   {{- range .Attributes }}
   {{- if .Example }}
   {{- if eq .Type "string" }}
    {{ .AttrName }} = "{{ .Example }}"
   {{- else }}
    {{ .AttrName }} = {{ .Example }}
   {{- end -}}
    {{ end }}
    {{- end }}
  }
}

